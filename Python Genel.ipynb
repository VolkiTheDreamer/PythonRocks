   "cell_type": "code",
   "execution_count": 192,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "5 ----------\n",
      "dict_keys(['one', 'two'])\n",
      " \n",
      "6 ----------\n",
      "dict_values(['bir', 'zwei'])\n",
      " \n",
      "7 ----------\n",
      "dict_items([('one', 'bir'), ('two', 'zwei')])\n",
      " \n",
      "bir\n",
      "N/A\n"
     ]
    }
   ],
