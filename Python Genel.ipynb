   "source": [
    "items=[1,2,3,4,5]\n",
    "def kareal(sayı):\n",
    "    return sayı**2\n",
    "\n",
    "kareler=map(kareal,items) #lambdalı da olur. map(lambda x: x**2, items)\n",
    "list(kareler) #yazdırmak için liste çevir\n"
